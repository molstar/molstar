    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "benchmark": "^2.1.4",
    "concurrently": "^8.2.2",
    "cpx2": "^7.0.1",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "extra-watch-webpack-plugin": "^1.0.3",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.2.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jpeg-js": "^0.4.4",
    "mini-css-extract-plugin": "^2.8.0",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
